        <Param val="// ========== 初始化按键变量 ==========&amp;#10;Key_pressed = false;&amp;#10;Key_release = false;&amp;#10;pressStart = null;&amp;#10;releaseTime = null;&amp;#10;&amp;#10;&amp;#10;// ========== 清理 PsychoJS 默认按键管理器，防止监听器冲突 ==========&amp;#10;if (typeof psychoJS !== 'undefined') {&amp;#10;  psychoJS.eventManager.clearEvents();&amp;#10;}&amp;#10;&amp;#10;// ========== 强制聚焦，确保能接收到键盘事件 ==========&amp;#10;window.focus();&amp;#10;console.log(&quot;✅ 强制 window 获取键盘焦点&quot;);&amp;#10;&amp;#10;// ========== 创建 RDK canvas ==========&amp;#10;const root = document.getElementById(&quot;root&quot;) || document.body;&amp;#10;if (!window.rdkCanvas) {&amp;#10;  const canvas = document.createElement(&quot;canvas&quot;);&amp;#10;  canvas.id = &quot;canvas&quot;;&amp;#10;  canvas.width = psychoJS.window.size[0];&amp;#10;  canvas.height = psychoJS.window.size[1];&amp;#10;  canvas.style.position = &quot;absolute&quot;;&amp;#10;  canvas.style.top = &quot;0&quot;;&amp;#10;  canvas.style.left = &quot;0&quot;;&amp;#10;  canvas.style.zIndex = &quot;10&quot;;&amp;#10;  canvas.style.backgroundColor = &quot;transparent&quot;;&amp;#10;  root.appendChild(canvas);&amp;#10;  window.rdkCanvas = canvas;&amp;#10;let cm_to_px_x = psychoJS.window.size[0] * x_scale;  // 每 cm 多少 px&amp;#10;let cm_to_px_y = psychoJS.window.size[1] * y_scale;&amp;#10;&amp;#10;  // ✅ 初始化惯性 RDK（inertial 模式）&amp;#10;  window.rdk = new RDK({&amp;#10;    canvasId: &quot;canvas&quot;,&amp;#10;    type: &quot;keypress&quot;,&amp;#10;    noiseMode: &quot;inertial&quot;,     // 🚀 支持惯性噪声&amp;#10;    direction: parseFloat(DirectionAngle),  // 替换为你的变量&amp;#10;    coherence: parseFloat(Coherence1),    // 替换为你的变量&amp;#10;    dotColor: &quot;rgb(7, 193, 7)&quot;,&amp;#10;    nDots: 360,&amp;#10;    speed: 0,                  // 初始静止&amp;#10;     dotSize: 0.03 * cm_to_px_x ,      // ✅ 点大小（单位像素）&amp;#10;  fieldRadius: 3.5 * cm_to_px_x // ✅ 圆半径（15cm直径的一半）&amp;#10;    &amp;#10;  });&amp;#10;&amp;#10;  console.log(&quot;🎬 Response RD